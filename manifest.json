{
    "name": "tagger",
    "version": "0.1.0",
    "description": "",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}